{
  "manifest_version": 3,
  "name": "shutup",
  "version": "1.0.0",
  "description": "Auto-closes Adblock Plus update / first-run tabs.",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_title": "shutup"
  }
}
